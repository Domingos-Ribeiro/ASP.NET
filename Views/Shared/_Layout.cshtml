<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Domingos S.A.", "Index", "Inicio", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Equipas", "Index", "Equipas", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Membros", "Index", "Membroes", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("A", "Index", "A", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("B", "Index", "B", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("C", "Index", "C", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("D", "Index", "D", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("E", "Index", "E", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("F", "Index", "F", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("G", "Index", "G", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("H", "Index", "H", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("I", "Index", "I", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("J", "Index", "J", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("K", "Index", "K", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("S1", "Index", "S1", new { area = "" }, new { @class = "navbar-brand" })

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>